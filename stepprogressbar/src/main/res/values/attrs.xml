<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StepProgressBar">
        <attr name="sp_space" format="integer"/>
        <attr name="sp_steps" format="integer"/>
        <attr name="sp_roundCorners" format="boolean"/>
        <attr name="sp_progressColor" format="color"/>
        <attr name="sp_progressBackgroundColor" format="color"/>
        <attr name="sp_progressWidth" format="dimension"/>
        <attr name="sp_progressBackgroundWidth" format="dimension"/>
        <attr name="sp_progress" format="float"/>
        <attr name="sp_currentStep" format="integer"/>
    </declare-styleable>
    <declare-styleable name="LineStepProgressBar">
        <attr name="lsp_space" format="dimension"/>
        <attr name="lsp_steps" format="integer"/>
        <attr name="lsp_roundCorners" format="boolean"/>
        <attr name="lsp_progressColor" format="color"/>
        <attr name="lsp_progressBackgroundColor" format="color"/>
        <attr name="lsp_progressWidth" format="dimension"/>
        <attr name="lsp_progressBackgroundWidth" format="dimension"/>
        <attr name="lsp_progress" format="float"/>
        <attr name="lsp_currentStep" format="integer"/>
    </declare-styleable>
</resources>